    const socket = io.connect();

    function setWs() {
      socket.on('new data', function (msg) {  // when get data from the socket
        // document.getElementById('totalCars').innerHTML=msg;
        console.log(msg);
        document.getElementById('S0').innerHTML = msg['1']+msg['2']+msg['3']+msg['4']+msg['5'];
        document.getElementById('S1').innerHTML = msg['1'];
        document.getElementById('S2').innerHTML = msg['2'];
        document.getElementById('S3').innerHTML = msg['3'];
        document.getElementById('S4').innerHTML = msg['4'];
        document.getElementById('S5').innerHTML = msg['5'];
      });

